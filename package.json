{
  "name": "webpack-yarn",
  "version": "0.0.1",
  "description": "Webpack FrontEnd starter with yarn",
  "main": "index.js",
  "scripts": {
    "dev-local": "webpack-dev-server --hot --progress --colors --config=webpack_config/dev.config.js",
    "dev": "webpack --mode=development --watch --progress --config=webpack_config/dev.config.js",
    "build": "webpack --mode=none --progress --config=webpack_config/build.config.js",
    "deploy": "gh-pages -d build",
    "prod": "webpack -p --mode=production --progress --config=webpack_config/build.config.js"
  },
  "author": "Diwanee",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.0",
    "babel-loader": "7",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-hot-loader": "^1.3.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "gh-pages": "^2.2.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "jquery": "^3.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.25.1",
    "webpack-build-notifier": "^2.0.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.10"
  }
}
